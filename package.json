{
  "name": "css-uwindsor-bot",
  "version": "2.0.0",
  "description": "Bot for UWindsor CSS discord",
  "main": "index.ts",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "start": "ts-node -r tsconfig-paths/register src/index.ts"
  },
  "author": "CSS Software Devs css@uwindsor.ca",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "bufferutil": "^4.0.5",
    "discord-api-types": "^0.37.51",
    "discord.js": "^14.12.1",
    "fuse.js": "^6.5.3",
    "js-yaml": "^4.1.0",
    "mathjax-node": "^2.1.1",
    "node-fetch": "^3.2.4",
    "pg": "^8.8.0",
    "pino": "^8.14.2",
    "svg2img": "^1.0.0-beta.2",
    "utf-8-validate": "^6.0.3"
  },
  "repository": "git@github.com:UWindsorCSS/uwindsor-discord-bot.git",
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "eslintConfig": {
    "node": true,
    "es6": true
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/mathjax": "^0.0.37",
    "@types/node": "^20.4.5",
    "@types/pg": "^8.6.5",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
