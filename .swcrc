{
    "jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": true,
			"dynamicImport": true
		},
		"target": "es2021",
		"baseUrl": "./",
        "paths": {
            "@/*": ["src/*"]
          },
		"transform": {
			"decoratorMetadata": true
		}
    },
	"exclude": ["node_modules", ".git"],
    "module": {
        "type": "commonjs"
    }
}